<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.LandDetails">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="257dp"
        android:layout_height="37dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="20000/Month"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="388dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="255dp"
        android:layout_height="39dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Geographic Information"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="454dp" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#40C53B"
        android:gravity="center"
        android:orientation="horizontal"
        android:padding="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975">

        <ImageView
            android:id="@+id/homeLink"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginRight="12dp"
            android:layout_weight="1"
            android:background="#64DD17"
            android:contentDescription="Home Link"
            app:srcCompat="@drawable/home" />

        <ImageView
            android:id="@+id/exploreLink"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:background="#40C53B"
            android:contentDescription="Explore Link"
            app:srcCompat="@drawable/search" />

        <ImageView
            android:id="@+id/dashboardLink"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginRight="12dp"
            android:layout_weight="1"
            android:contentDescription="Dashboard Link"
            app:srcCompat="@drawable/post" />

        <ImageView
            android:id="@+id/profileLink"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:contentDescription="Profile Link"
            app:srcCompat="@drawable/user" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Land Details"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.192"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView2"
        android:layout_width="197dp"
        android:layout_height="156dp"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="121dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="159dp"
        android:layout_height="151dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="121dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="257dp"
        android:layout_height="37dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="50 Cents Agriculture Land"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="318dp" />

    <LinearLayout
        android:layout_width="400dp"
        android:layout_height="81dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.754"
        tools:layout_editor_absoluteX="2dp">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="118dp"
            android:layout_height="37dp"
            android:layout_weight="1"
            android:text="Elavation"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="138dp"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:text="Solar(Light)"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="159dp"
            android:layout_height="34dp"
            android:layout_weight="1"
            android:text="Polution Impact"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="406dp"
        android:layout_height="98dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="525dp">

        <TextView
            android:id="@+id/textView19"
            android:layout_width="0dp"
            android:layout_height="61dp"
            android:layout_weight="1"
            android:text="1800ft" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="1dp"
            android:layout_height="65dp"
            android:layout_weight="1"
            android:text="Lumens" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:layout_weight="1"
            android:text="High" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>